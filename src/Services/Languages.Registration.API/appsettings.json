{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "ApplicationInsights",
        "Args": {
          "connectionString": "InstrumentationKey=...",
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "ApplicationName": "Languages.Registration.API"
    }
  },
  "AllowedHosts": "",
  "ConnectionStrings": {
    "MongoDb": ""
  },
  "Jwt": {
    "Issuer": "",
    "Audience": "",
    "SecretKey": ""
  },
  "BlobStorage": {
    "ConnectionString": "x",
    "ContainerName": "x"
  }
}
